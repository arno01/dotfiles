[user]
        name = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %>
        email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[push]
	default = upstream
[color]
	ui = true
	branch = true
	diff = true
	interactive = true
	status = true
[branch]
	autosetuprebase = always
[core]
	excludesfile = ~/.gitignore
	precomposeunicode = true
[alias]
	co = checkout
	pr = pull --rebase
	pu = push
	cm = commit -v
